{
  "name": "alcyz/gx-selenium-framework",
  "description": "php selenium framework",
  "version": "1.0.0",
  "license": "MIT",
  "require": {
    "php": ">=5.4.0",
    "facebook/webdriver": "~1.0",
    "AlcyZ/alcys-orm": "dev-master"
  },
  "repositories": [
    {
      "type": "vcs",
      "url": "https://github.com/AlcyZ/Alcys-ORM.git"
    }
  ],
  "autoload": {
    "psr-4": {
      "GXSelenium\\": "src/"
    }
  },
  "scripts": {
    "pre-update-cmd": [
      "cp -r ./logs ./../../logs_temp"
    ],
    "post-update-cmd": [
      "cp -r ./../../logs_temp/* ./logs/.",
      "rm -rf ./../../logs_temp"
    ]
  }
}
